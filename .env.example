# Environment Configuration
NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=FreshConnect
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Professional marketplace for street food vendors and suppliers"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_RAZORPAY_KEY_ID=your_razorpay_key_here
VITE_FIREBASE_API_KEY=your_firebase_api_key_here

# Monitoring & Analytics
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Business Configuration
VITE_DEFAULT_DELIVERY_RADIUS=10
VITE_MIN_ORDER_AMOUNT=100
VITE_MAX_ORDER_AMOUNT=50000
VITE_SUPPORT_EMAIL=support@freshconnect.com
VITE_SUPPORT_PHONE=+91-9876543210

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_OFFLINE_CACHE_SIZE=50

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_RATE_LIMITING=true
